<%= form_for(@entry) do |f| %>
  <div class="field">
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.text_field :title, placeholder: "Title", class: "form-control" %>
    <%= f.text_area :content, placeholder: "Where are you thinking?", class: "form-control" %>
  </div>
  <%= f.submit "to Post", class: "btn btn-primary"%>
<% end %>
